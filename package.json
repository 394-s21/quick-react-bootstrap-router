{
  "name": "quick-react-bootstrap-router",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-query-firebase/auth": "^0.3.4",
    "@react-query-firebase/database": "^0.2.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "bootstrap": "^5.0.2",
    "firebase": "^9.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^3.0.4",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.3.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cy:open": "cypress open",
    "cy:run": "cypress run --video=false",
    "em:start": "firebase emulators:start --import=./saved-data --export-on-exit",
    "em:exec": "REACT_APP_EMULATE=true firebase emulators:exec --ui --import=./saved-data 'npm start'",
    "em:cy:open": "start-server-and-test em:exec http://127.0.0.1:3000 cy:open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "resetMocks": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^9.7.0",
    "react-scripts": "^5.0.1",
    "start-server-and-test": "^1.14.0"
  }
}
